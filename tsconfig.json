{
    "compilerOptions": {
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "strict": true,
      "allowJs": false,
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "ES2018",
      "forceConsistentCasingInFileNames": true,
      "outDir": "dist",
      "resolveJsonModule": true
    }
}
